{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "cloudflare-image-uploader",
  "title": "Cloudflare Image Uploader",
  "description": "Upload Image to CloudFlare Image Service",
  "icon": "command-icon.png",
  "author": "jinzhubaofu",
  "categories": [
    "Media"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Upload",
      "description": "Upload your image to Cloudflare Image Service",
      "mode": "view"
    }
  ],
  "preferences": [
    {
      "name": "token",
      "description": "cloudflare token",
      "type": "textfield",
      "title": "cloudflare token",
      "required": true
    },
    {
      "name": "accountId",
      "type": "textfield",
      "title": "cloudflare account id",
      "description": "cloudflare account id",
      "required": true
    },
    {
      "name": "accountHash",
      "type": "textfield",
      "title": "cloudflare account hash",
      "description": "cloudflare account hash",
      "required": true
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.64.5"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/node": "20.8.10",
    "@types/react": "18.2.27",
    "eslint": "^8.51.0",
    "prettier": "^3.1.1",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
