{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "imagekit-uploader",
  "title": "imagekit Uploader",
  "description": "Upload Image to imagekit",
  "icon": "command-icon.png",
  "author": "jinzhubaofu",
  "categories": [
    "Media"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Upload",
      "description": "Upload images to ImageKit",
      "mode": "view"
    }
  ],
  "preferences": [
    {
      "name": "publicKey",
      "description": "Public Key",
      "type": "textfield",
      "title": "Public Key",
      "required": true
    },
    {
      "name": "privateKey",
      "type": "password",
      "title": "Private Key",
      "description": "Private Key",
      "required": true
    },
    {
      "name": "urlEndpoint",
      "type": "textfield",
      "title": "Url Endpoint",
      "description": "Url Endpoint",
      "required": true
    }
  ],
  "dependencies": {
    "@prisma/client": "^5.7.1",
    "@raycast/api": "^1.64.5",
    "@raycast/utils": "^1.10.1",
    "image-type": "^5.2.0",
    "imagekit": "^4.1.4",
    "read-chunk": "^4.0.3",
    "run-applescript": "^7.0.0",
    "sqlite": "^5.1.1"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/node": "20.8.10",
    "@types/react": "18.2.27",
    "dotenv-cli": "^7.3.0",
    "eslint": "^8.51.0",
    "prettier": "^3.1.1",
    "prisma": "^5.7.1",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
